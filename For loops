#challenge three--------------------------------------------------------------------------------------
name = str(input("Input your name>>>"))
for i in range(2):
  print(name)
#challenge four--------------------------------------------------------------------------------------
number = int(input("Enter a times table: "))
for i in range(1,13):
  print("showing the",number,"times table")
  print(i ,"x", number, "=", i+number)
#challenge five--------------------------------------------------------------------------------------
for i in range(10, 0, -1):
  print (i)
print("!!Blast off!!")
#challenge six--------------------------------------------------------------------------------------
rainbow =["red","orange","yellow","green","blue","indigo", "violet"]
number = 0 
for i in rainbow:
  number = number + 1
  print ("Colour",number*2,"is", i)
#challenge seven--------------------------------------------------------------------------------------
print("Write a digi")
x = int(input())
for i in range(x, 0, -1):
  print(i)
#challenge eight--------------------------------------------------------------------------------------
import random
for i in range(5):
  a = random.randint(0, 50)
  print(a)
#challenge nine--------------------------------------------------------------------------------------
import random

money = [0, 1, 10, 100, 1000, 10000,]
mselected = random.choice(money)

for i in range(1,9):
  print("player",i,"gets",mselected,"extra" )
  mselected = random.choice(money)
#challenge ten------------------------------------------------------------------------------------------
numHouses = int(input("how many houses?>>> "))
for i in range(1, numHouses+1):
  print("for house",i)
  numWalls = int(input("How many walls?>>> "))
  for i in range(1, numWalls+1):
    print("for wall",i)
    col = str(input("what colour is it?>>> "))
